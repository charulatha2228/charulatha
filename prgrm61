# Create a dictionary
my_dict = {
    "name": "Alice",
    "age": 25,
    "city": "New York",
    "profession": "Engineer"
}

# Print the dictionary's keys and values
print("Keys:")
for key in my_dict.keys():
    print(key)

print("\nValues:")
for value in my_dict.values():
    print(value)

print("\nKeys and Values:")
for key, value in my_dict.items():
    print(f"{key}: {value}")
