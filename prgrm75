
# Define the function to calculate factorial
def factorial(n):
    # Initialize result
    result = 1
    # Multiply result by each number from 1 to n
    for i in range(1, n + 1):
        result *= i
    return result

# Call the function with a number
num = 5
result = factorial(num)

# Print the result
print(f"The factorial of {num} is {result}")
