import matplotlib.pyplot as plt
import numpy as np

# Generate random data points
x = np.random.rand(50)  # 50 random values for x-axis
y = np.random.rand(50)  # 50 random values for y-axis

# Create a scatter plot
plt.scatter(x, y, color='blue', marker='o')  # Customize color and marker style

# Add title and labels
plt.title("Scatter Plot of Random Data Points")
plt.xlabel("X-axis")
plt.ylabel("Y-axis")

# Display the plot
plt.show()
