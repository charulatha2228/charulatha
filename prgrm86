import numpy as np

# Define two matrices A and B
A = np.array([[1, 2], 
              [3, 4]])

B = np.array([[5, 6], 
              [7, 8]])

# Perform matrix multiplication using np.dot() or the @ operator
result = np.dot(A, B)  # Alternatively, you can use A @ B for matrix multiplication

# Print the result
print("Matrix A:")
print(A)
print("\nMatrix B:")
print(B)
print("\nMatrix multiplication result:")
print(result)
