# Define the recursive function to find the nth Fibonacci number
def fibonacci(n):
    # Base cases
    if n <= 1:
        return n
    # Recursive case
    else:
        return fibonacci(n - 1) + fibonacci(n - 2)

# Call the function to print the Fibonacci sequence up to the nth term
n = 10  # Find the first 10 Fibonacci numbers
for i in range(n):
    print(fibonacci(i), end=" ")
