# Define the function to find the largest of three numbers
def find_largest(num1, num2, num3):
    # Compare the numbers and return the largest
    if num1 >= num2 and num1 >= num3:
        return num1
    elif num2 >= num1 and num2 >= num3:
        return num2
    else:
        return num3

# Call the function with three numbers
num1 = 10
num2 = 20
num3 = 15
largest = find_largest(num1, num2, num3)

# Print the result
print(f"The largest of {num1}, {num2}, and {num3} is {largest}")
